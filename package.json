{
  "name": "youtube-offline",
  "version": "1.0.0",
  "description": "YouTube playlist downloader with periodic sync and web interface",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js",
