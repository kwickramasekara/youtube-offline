    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3"
  }
}
